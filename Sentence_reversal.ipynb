    "def sentenceReverse(sentence):\n",
    "    \n",
    "    sentence = sentence.split(\" \")\n",
    "    new_str = sentence[-1::-1]\n",
    "    \n",
    "    output = ' '.join(new_str)\n",
    "    \n",
    "    return output"
   ]
